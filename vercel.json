{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/Popular-movies",  
        "dest": "/index.js"
      },
      {
        "src": "/api/Popular-movies/(.*)",  
        "dest": "/index.js"
      },
      {
        "src": "/api/movies", 
        "dest": "/index.js"
      }
    ]
  }

  
  